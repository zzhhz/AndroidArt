<?xml version="1.0" encoding="utf-8"?>
<com.zzh.event.dispatch.TouchActivityGroup
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zzh.event.MainActivity">

    <com.zzh.event.dispatch.TouchViewGroup
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerInParent="true"
        android:background="#f6b055">

        <com.zzh.event.dispatch.TouchView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerInParent="true"
            android:background="#0095ff"/>

    </com.zzh.event.dispatch.TouchViewGroup>

    <ImageView
        android:id="@+id/iv"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentBottom="true"
        android:background="#0095ff"/>

</com.zzh.event.dispatch.TouchActivityGroup>
